<odoo>
    <record id="report_mitra_xlsx" model="ir.actions.report">
        <field name="name">Report Mitra Excel</field>
        <field name="model">hayatbookstore.mitra</field>
        <field name="report_type">xlsx</field>
        <field name="report_name">hayatbookstore.report_mitra_xlsx</field>
        <field name="report_file">hayatbookstore.report_mitra_xlsx</field>
        <field name="binding_model_id" ref="model_hayatbookstore_mitra"/>
        <field name="binding_type">report</field>
    </record>
    
    <record id="report_transaksi_xlsx" model="ir.actions.report">
        <field name="name">Penjualan Excel</field>
        <field name="model">hayatbookstore.penjualan</field>
        <field name="report_type">xlsx</field>
        <field name="report_name">hayatbookstore.report_transaksi_xlsx</field>
        <field name="report_file">hayatbookstore.report_transaksi_xlsx</field>
        <field name="binding_model_id" ref="model_hayatbookstore_penjualan"/>
        <field name="binding_type">report</field>
    </record>

    <record id="report_penjualan_pdf" model="ir.actions.report">
        <field name="name">Print Report as PDF</field>
        <field name="model">hayatbookstore.penjualan</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">hayatbookstore.report_penjualan_pdf</field>
        <field name="report_file">hayatbookstore.report_penjualan_pdf</field>
        <field name="binding_model_id" ref="model_hayatbookstore_penjualan"/>
        <field name="binding_type">report</field>
    </record> 

</odoo>
